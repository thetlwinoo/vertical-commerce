<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity CitiesCulture.
    -->
    <changeSet id="20200718073112-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="culture_id"
                                 baseTableName="cities_culture"
                                 constraintName="fk_cities_culture_culture_id"
                                 referencedColumnNames="id"
                                 referencedTableName="culture"/>

        <addForeignKeyConstraint baseColumnNames="city_id"
                                 baseTableName="cities_culture"
                                 constraintName="fk_cities_culture_city_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cities"/>

    </changeSet>
</databaseChangeLog>
