<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity CountriesCulture.
    -->
    <changeSet id="20200718072712-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="culture_id"
                                 baseTableName="countries_culture"
                                 constraintName="fk_countries_culture_culture_id"
                                 referencedColumnNames="id"
                                 referencedTableName="culture"/>

        <addForeignKeyConstraint baseColumnNames="country_id"
                                 baseTableName="countries_culture"
                                 constraintName="fk_countries_culture_country_id"
                                 referencedColumnNames="id"
                                 referencedTableName="countries"/>

    </changeSet>
</databaseChangeLog>
