<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity ProductBrand.
    -->
    <changeSet id="20200617164704-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="supplier_id"
                                 baseTableName="product_brand"
                                 constraintName="fk_product_brand_supplier_id"
                                 referencedColumnNames="id"
                                 referencedTableName="suppliers"/>

        <addForeignKeyConstraint baseColumnNames="icon_id"
                                 baseTableName="product_brand"
                                 constraintName="fk_product_brand_icon_id"
                                 referencedColumnNames="id"
                                 referencedTableName="photos"/>

    </changeSet>
</databaseChangeLog>
