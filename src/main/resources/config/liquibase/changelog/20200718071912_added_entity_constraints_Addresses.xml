<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Addresses.
    -->
    <changeSet id="20200718071912-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="region_id"
                                 baseTableName="addresses"
                                 constraintName="fk_addresses_region_id"
                                 referencedColumnNames="id"
                                 referencedTableName="regions"/>

        <addForeignKeyConstraint baseColumnNames="city_id"
                                 baseTableName="addresses"
                                 constraintName="fk_addresses_city_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cities"/>

        <addForeignKeyConstraint baseColumnNames="township_id"
                                 baseTableName="addresses"
                                 constraintName="fk_addresses_township_id"
                                 referencedColumnNames="id"
                                 referencedTableName="townships"/>

        <addForeignKeyConstraint baseColumnNames="town_id"
                                 baseTableName="addresses"
                                 constraintName="fk_addresses_town_id"
                                 referencedColumnNames="id"
                                 referencedTableName="towns"/>

        <addForeignKeyConstraint baseColumnNames="address_type_id"
                                 baseTableName="addresses"
                                 constraintName="fk_addresses_address_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="address_types"/>

        <addForeignKeyConstraint baseColumnNames="customer_address_id"
                                 baseTableName="addresses"
                                 constraintName="fk_addresses_customer_address_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customers"/>

        <addForeignKeyConstraint baseColumnNames="supplier_address_id"
                                 baseTableName="addresses"
                                 constraintName="fk_addresses_supplier_address_id"
                                 referencedColumnNames="id"
                                 referencedTableName="suppliers"/>

    </changeSet>
</databaseChangeLog>
