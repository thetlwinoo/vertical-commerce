<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity StockItemsCulture.
    -->
    <changeSet id="20200718082412-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="culture_id"
                                 baseTableName="stock_items_culture"
                                 constraintName="fk_stock_items_culture_culture_id"
                                 referencedColumnNames="id"
                                 referencedTableName="culture"/>

        <addForeignKeyConstraint baseColumnNames="stock_item_id"
                                 baseTableName="stock_items_culture"
                                 constraintName="fk_stock_items_culture_stock_item_id"
                                 referencedColumnNames="id"
                                 referencedTableName="stock_items"/>

    </changeSet>
</databaseChangeLog>
