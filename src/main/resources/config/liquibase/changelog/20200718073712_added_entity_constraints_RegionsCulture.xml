<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity RegionsCulture.
    -->
    <changeSet id="20200718073712-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="culture_id"
                                 baseTableName="regions_culture"
                                 constraintName="fk_regions_culture_culture_id"
                                 referencedColumnNames="id"
                                 referencedTableName="culture"/>

        <addForeignKeyConstraint baseColumnNames="region_id"
                                 baseTableName="regions_culture"
                                 constraintName="fk_regions_culture_region_id"
                                 referencedColumnNames="id"
                                 referencedTableName="regions"/>

    </changeSet>
</databaseChangeLog>
