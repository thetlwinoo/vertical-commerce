<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity ProductAttribute.
    -->
    <changeSet id="20200610151945-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="product_attribute_set_id"
                                 baseTableName="product_attribute"
                                 constraintName="fk_product_attribute_product_attribute_set_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product_attribute_set"/>

        <addForeignKeyConstraint baseColumnNames="supplier_id"
                                 baseTableName="product_attribute"
                                 constraintName="fk_product_attribute_supplier_id"
                                 referencedColumnNames="id"
                                 referencedTableName="suppliers"/>

    </changeSet>
</databaseChangeLog>
